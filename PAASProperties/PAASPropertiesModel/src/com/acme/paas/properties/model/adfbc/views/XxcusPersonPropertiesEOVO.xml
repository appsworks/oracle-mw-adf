  <SQLQuery>
    <![CDATA[SELECT XxcusPersonPropertiesEO.PERSON_PROPERTY_ID, 
       XxcusPersonPropertiesEO.PERSON_ID, 
       XxcusPersonPropertiesEO.PERSON_PROPERTY_TYPE, 
       XxcusPersonPropertiesEO.PERSON_PROPERTY_VALUE, 
       XxcusPersonPropertiesEO.STATUS, 
       XxcusPersonPropertiesEO.START_DATE_ACTIVE, 
       XxcusPersonPropertiesEO.END_DATE_ACTIVE, 
       XxcusPersonPropertiesEO.CREATED_BY, 
       XxcusPersonPropertiesEO.CREATION_DATE, 
       XxcusPersonPropertiesEO.LAST_UPDATED_BY, 
       XxcusPersonPropertiesEO.LAST_UPDATE_LOGIN, 
       XxcusPersonPropertiesEO.LAST_UPDATE_DATE, 
       XxcusPersonPropertiesEO.OBJECT_VERSION_NUMBER,
       e.first_name,
       e.last_name,
       e.first_name||' '||e.last_name full_name,
       lkp_ppt.meaning property_type_meaning,
       lkp_ppt.tag person_property_value_lov,
       lkp_ppv.meaning property_value,
       lkp_pps.meaning status_meaning
FROM XXCUS_PERSON_PROPERTIES XxcusPersonPropertiesEO
,    employees e
,    xxcus_lookup_values lkp_ppt
,    xxcus_lookup_values lkp_ppv
,    xxcus_lookup_values lkp_pps
WHERE XxcusPersonPropertiesEO.person_id = e.employee_id(+)
  AND XxcusPersonPropertiesEO.person_property_type = lkp_ppt.lookup_code(+)
  AND lkp_ppt.lookup_type(+) = 'PERSON_PROPERTIES'
  AND lkp_ppt.tag = lkp_ppv.lookup_type(+)
  AND lkp_pps.lookup_type(+) = 'PERSON_PROPERTY_STATUS'
  AND XxcusPersonPropertiesEO.status = lkp_pps.lookup_code(+)
  AND XxcusPersonPropertiesEO.person_property_value = lkp_ppv.lookup_code(+)]]>
  </SQLQuery>
