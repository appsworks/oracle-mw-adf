  <SQLQuery>
    <![CDATA[SELECT XxcusPersonProperties.PERSON_PROPERTY_ID,
       XxcusPersonProperties.PERSON_ID,
       XxcusPersonProperties.PERSON_PROPERTY_TYPE,
       XxcusPersonProperties.PERSON_PROPERTY_VALUE,
       XxcusPersonProperties.STATUS,
       XxcusPersonProperties.START_DATE_ACTIVE,
       XxcusPersonProperties.END_DATE_ACTIVE,
       XxcusPersonProperties.CREATED_BY,
       XxcusPersonProperties.CREATION_DATE,
       XxcusPersonProperties.LAST_UPDATED_BY,
       XxcusPersonProperties.LAST_UPDATE_LOGIN,
       XxcusPersonProperties.LAST_UPDATE_DATE,
       XxcusPersonProperties.OBJECT_VERSION_NUMBER,
       e.first_name,
       e.last_name,
       e.first_name||' '||e.last_name full_name,
       lkp_ppt.meaning property_type_meaning,
       lkp_ppt.tag person_property_value_lov,
       lkp_ppv.meaning property_value,
       lkp_pps.meaning status_meaning
FROM XXCUS_PERSON_PROPERTIES XxcusPersonProperties
,    employees e
,    xxcus_lookup_values lkp_ppt
,    xxcus_lookup_values lkp_ppv
,    xxcus_lookup_values lkp_pps
WHERE XxcusPersonProperties.person_id = e.employee_id
  AND XxcusPersonProperties.person_property_type = lkp_ppt.lookup_code
  AND lkp_ppt.lookup_type = 'PERSON_PROPERTIES'
  AND lkp_ppt.tag = lkp_ppv.lookup_type
  AND lkp_pps.lookup_type = 'PERSON_PROPERTY_STATUS'
  AND XxcusPersonProperties.status = lkp_pps.lookup_code
  AND XxcusPersonProperties.person_property_value = lkp_ppv.lookup_code]]>
  </SQLQuery>
